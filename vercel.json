{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://server-zang-bazar-football-club.vercel.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}